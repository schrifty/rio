<!DOCTYPE html>
<html lang="en">
<title>Quinstantly</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<head>
  <link href="/assets/bootstrap.min.css" rel="stylesheet">
  <script src="/assets/bootstrap.min.js"></script>
  <link href="/assets/font-awesome-3.2.1.css" rel="stylesheet">
  <div id="header">
    <div class="row">
      <hgroup>
        <h1>Quinstantly!</h1>
      </hgroup>
    </div>
  </div>
</head>
<body>
<div id="tenant-main">
  <div id="login">

    <!-- SIGN IN -->
    <div class="row">
      <input id="email" class="col-lg-4 col-offset-4" type="text" placeholder="Email"/>
    </div>
    <div class="row passwords" id="password-div">
      <input id="password" class="col-lg-4 col-offset-4" type="password" placeholder="Password"/>
    </div>
    <div class="row" id="signin-button-div">
      <button class="col-lg-4 col-offset-4 btn btn-large btn-primary" id="signin-button" disabled="disabled">
        SIGN IN
      </button>
    </div>
    <div class="row" id="signin-spinner">
      <i class="icon-spinner icon-spin icon-large col-lg-4 col-offset-4"></i>
    </div>

    <!-- SIGN UP -->
    <div class="row">
      <input id="email-new" class="col-lg-4 col-offset-4" type="text" placeholder="Email"/>
    </div>
    <div class="row passwords">
      <input id="password-new" class="col-lg-4 col-offset-4" type="password" placeholder="Password"/>
    </div>
    <div class="row passwords">
      <input id="password-confirm" class="col-lg-4 col-offset-4" type="password" placeholder="Password"/>
    </div>
    <div class="row" id="signup-button-div">
      <button class="col-lg-4 col-offset-4 btn btn-large btn-primary" id="signup-button" disabled="disabled">
        SIGN UP
      </button>
    </div>
    <div class="row" id="signup-spinner">
      <i class="icon-spinner icon-spin icon-large col-lg-4 col-offset-4"></i>
    </div>
  </div>
</div>
<div id="#main">

</div>
<div id="#profile">

</div>
</body>
</html>